{
  "manifest_version": 3,
  "name": "mBank DOM Dumper",
  "version": "1.0.0",
  "description": "Dump DOM before refresh on mBank CompanyNet",

  "content_scripts": [
    {
      "matches": ["https://companynet.mbank.pl/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
